    <string name="title_contact_probe">Contact Probe</string>
    <string name="summary_contact_probe_desc">Detailed information about the contacts available to the device. Sensitive information is one way hashed to ensure user privacy.</string>

    <string name="probe_contact_full_label">Use Full Contact List</string>
    <string name="probe_contact_full_summary">If \'yes\', return all contacts, otherwise return contacts that have changed since the most recent scan.</string>

    <string name="title_call_log_probe">Call Log Probe</string>
    <string name="summary_call_log_probe_desc">Records the calls that are made by the device. Sensitive information is normalized and hashed consistently and can be compared to contacts on this device, or with other devices.</string>

    <string name="title_sms_probe">SMS Probe</string>
    <string name="summary_sms_probe_desc">Messages sent and received by this device using SMS. Sensitive data is hashed for user privacy.</string>

    <string name="title_activity_probe">Activity Probe</string>
    <string name="summary_activity_probe_desc">Records how active the person is. Uses the AccelerometerProbe data to calculate how many intervals the variance of a device\'s acceleration is above a certain threshold.</string>

    <string name="title_screen_probe">Screen Probe</string>
    <string name="summary_screen_probe_desc">Records when the screen turns off and on.</string>

    <string name="title_videos_probe">Videos Probe</string>
    <string name="summary_videos_probe_desc">Information about video files on the device.</string>

    <string name="title_audio_files_probe">Audio Files Probe</string>
    <string name="summary_audio_files_probe_desc">Information about audio files on the device.</string>

    <string name="title_image_files_probe">Image Files Probe</string>
    <string name="summary_image_files_probe_desc">Information about image files on the device.</string>

    <string name="title_browser_searches_probe">Browser Searches Probe</string>
    <string name="summary_browser_searches_probe_desc">Records the searches made in the browser.</string>

    <string name="title_browser_bookmarks_probe">Browser Bookmarks Probe</string>
    <string name="summary_browser_bookmarks_probe_desc">Records the bookmarks stored in the browser.</string>

    <string name="title_android_info_probe">Android Information Probe</string>
    <string name="summary_android_info_probe_desc">Information about the version of Android the device is running.</string>

    <string name="title_battery_probe">Battery Probe</string>
    <string name="summary_battery_probe_desc">Information about the type and current state of the battery in the device.</string>

    <string name="title_hardware_info_probe">Hardware Information Probe</string>
    <string name="summary_hardware_info_probe_desc">Details about the specific hardware the device is running, including component identifiers.</string>

    <string name="title_installed_applications_probe">Installed Applications Probe</string>
    <string name="summary_installed_applications_probe_desc">What applications are installed on the device. Also specifies applications that were uninstalled.</string>

    <string name="title_running_applications_probe">Running Applications Probe</string>
    <string name="summary_running_applications_probe_desc">The current running stack of applications.</string>

    <string name="title_light_probe">Light Probe</string>
    <string name="summary_light_probe_desc">Detects the ambient light level in SI lux units.</string>

    <string name="title_magnetic_field_probe">Magnetic Field Probe</string>
    <string name="summary_magnetic_field_probe_desc">Detect the local magnetic field. Can be used as to detect the earth\'s magnetic field, but may be distorted by metal objects around device.</string>

    <string name="title_pressure_probe">Pressure Probe</string>
    <string name="summary_pressure_probe_desc">Records the pressure on the touch screen of the devices.</string>

    <string name="title_proximity_probe">Proximity Probe</string>
    <string name="summary_proximity_probe_desc">How far the front of the device is from an object.</string>

    <string name="title_telephony_probe">Telephony Probe</string>
    <string name="summary_telephony_probe_desc">Records telephony hardware, software, and account information.</string>

    <string name="title_temperature_probe">Temperature Probe</string>
    <string name="summary_temperature_probe_desc">Used to record temperature. Implementation depends on the device and does not exist on all devices. Some will record temperature of battery, others temperature of CPU or environment.</string>

    <string name="title_time_offset_probe">Time Offset Probe</string>
    <string name="summary_time_offset_probe_desc">Checks NTP servers to determine the devices current offset from the time servers.</string>

    <string name="title_probe_readings">Status</string>

    <string name="title_builtin_pressure_probe">Pressure Probe (Built-in)</string>
    <string name="summary_builtin_pressure_probe_desc">Uses a native sensor to record pressure readings continuously.</string>

    <string name="title_builtin_accelerometer_probe">Accelerometer Probe (Built-in)</string>
    <string name="summary_builtin_accelerometer_probe_desc">Uses a native acceletmeter to record acceleration vector continuously.</string>

    <string name="title_builtin_gyroscope_probe">Gyroscope Probe (Built-in)</string>
    <string name="summary_builtin_gyroscope_probe_desc">Measures angular speed around each axis.</string>

    <string name="title_builtin_magnetic_probe">Magnetic Field Probe (Built-in)</string>
    <string name="summary_builtin_magnetic_field_probe_desc">Detect the local magnetic field. Can be used as to detect the earth\'s magnetic field, but may be distorted by metal objects around device.</string>

