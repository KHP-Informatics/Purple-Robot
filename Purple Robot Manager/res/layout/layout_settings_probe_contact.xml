<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:holo="http://schemas.android.com/apk/res-auto"
    holo:title="@string/title_contact_probe"
    holo:summary="@string/summary_contact_probe_desc">
   	<CheckBoxPreference
		holo:title="@string/title_enable_probe"
		holo:key="config_probe_contact_enabled"
		holo:defaultValue="true"/>
    <ListPreference
        holo:key="config_probe_contact_period"
        holo:entries="@array/probe_period_labels"
        holo:entryValues="@array/probe_period_values"
        holo:title="@string/probe_period_label"
        holo:summary="@string/probe_period_summary"
        holo:defaultValue="1200" />
    <ListPreference
        holo:key="config_probe_contact_full_list"
        holo:entries="@array/probe_boolean_labels"
        holo:entryValues="@array/probe_boolean_values"
        holo:title="@string/probe_contact_full_label"
        holo:summary="@string/probe_contact_full_summary"
        holo:defaultValue="false" />
</PreferenceScreen>