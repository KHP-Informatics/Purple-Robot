<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:holo="http://schemas.android.com/apk/res-auto"
    holo:title=""
    holo:summary="">
   	<CheckBoxPreference
		holo:title="@string/title_enable_probe"
		holo:key="config_probe_funf_enabled"
		holo:defaultValue="true"/>
    <ListPreference
        holo:key="config_probe_funf_period"
        holo:entries="@array/probe_period_labels"
        holo:entryValues="@array/probe_period_values"
        holo:title="@string/probe_period_label"
        holo:summary="@string/probe_period_summary"
        holo:defaultValue="1800" />
    <ListPreference
        holo:key="config_probe_funf_duration"
        holo:entries="@array/probe_duration_labels"
        holo:entryValues="@array/probe_duration_values"
        holo:title="@string/probe_duration_label"
        holo:summary="@string/probe_duration_summary"
        holo:defaultValue="120" />
</PreferenceScreen>